{
    "bracketSpacing": true,
    "printWidth": 100,
    "tabWidth": 4,
    "trailingComma": "es5",
    "singleAttributePerLine": true,
    "semi": true,
    "importOrder": [
        "^react(.*)$",
        "antd(.*)$",
        "<THIRD_PARTY_MODULES>",
        "@/(.*)",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "singleQuote": false
}